//To check if sum of digits is palindrome.
#include <iostream>
using namespace std;

int main() {
	//code
	
	int t;
	cin>>t;
	
	while(t>0)
	{
	    int n;
	    cin>>n; //n=121
	    
	    int a=0;
	    while(n>0)
	    {
	        a=a+n%10; 
	        n=n/10;  
	    }
	    
	    int sum=a;int rev=0;   
	    
	    while(a>0)
	    {
	        rev=rev*10+a%10; 
	        a=a/10;          
	    }
                  //we need to compare the sum with its reverse and not a since a eventually will become 0 always
	   if(sum==rev) //------------------------------Here is the Change-------------------------------
	    cout<<"YES";
	    else
	    cout<<"NO";
	   
	   t=t-1; 
	}
	
	return 0;
}
